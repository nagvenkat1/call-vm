pipeline {
    agent any
        stages {
        stage('vcs') {
            steps {
                git branch: 'main', url: 'https://github.com/nagvenkat1/createvm.git'
            }
        }
         stage('create') {
            steps {
                sh 'chmod 777 vmcli.sh'
                sh './vmcli.sh'
                
            }
         }
     }

}
